/* cosmetic changes for ASKB */
body {
    background: #782e29;
}

#wrapper
{
    background: #edddcb;
    border:none;
    overflow:auto;
}

div.floatright
{
	border:1px solid;
	float:right;
	width: 300px;
	background: #ffffff;
	margin-left: 5px;
	margin-bottom: 5px;
}
h3
{
        text-align: left;
        color: #752b27;
        padding-bottom: 4px;
	padding-left: 4px;
}

#header
{
        padding-top: 10px;
        background-color:#782e29;
        border:none;
}

#navbar {
    width: 110px;
    float: left;
    padding-left: 14px;
    padding-right: 10px;
    padding-top: 25px;
    padding-bottom: 0px;
    margin-top: 20px;
    display : block ;
}

#contact {
    padding-top: 50px;
    padding-bottom: 12px;
}

#main {
    width: 616px;
    float: left;
    padding-left: 15px;
    padding-right: 15px;
    padding-top: 0px;
    padding-bottom: 0px;
    margin-top: 0px;
    border-left: 0px dotted #000;
    margin-bottom: 0px;
    border-right: 0px dotted #000;
}

#main p {
    line-height: 16px;
    margin-top: 5px;
    margin-bottom: 1em;
}

#main h2 {
	text-align: left;
	color: #752b27;
	border-top: 1px solid #ba9593;
	border-bottom: 1px solid #ba9593;
	padding-top: 4px;
	padding-bottom: 4px;
}

#searchform h2 {
        text-align: left;
        color: #752b27;
        padding-top: 4px;
        padding-bottom: 4px;
}

#footer p {
    text-align: center;
    margin: 1em 16px;
    padding-top: 9px;
    padding-bottom: 6px;
    font-weight: bold;
    text-transform: uppercase;
    font-size: 9px;
    background-color: transparent;
    color: #3a1e1a;
}

a:link, a:visited {
    font-family: Arial, sans-serif;
    text-decoration: none;
    color: #752b27;
    background: transparent;
}

a:hover {
    text-decoration: underline;
    color: #752b27;
    background: transparent;
}

.imagefloatleft {
	float: left;
	text-align: left;
	margin-right: 16px;
	margin-bottom: 12px;
	color: #752b27;
}

.imagefloatright {
	float: right;
	text-align: right;
	margin-bottom: 16px;
	margin-left: 16px;
	color: #752b27;
}

#navbar ul li,
#navbar ul#servicelink li {
    margin: 0;
    font: 11px Arial, sans-serif;
    margin-top: 10px;
    margin-bottom: 21px;
    font-weight: bold;
    text-transform: uppercase;
    line-height: 120%;
}

div.indent
{
	margin-left:15px;
	margin-bottom:2px;
	font-size: 10px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

#searchbox
{
	background: #edddcb;
	width: 575px;
	border: 1px solid #752b27;
	padding: 10px;
	margin-bottom: 10px;
	overflow: auto;
}

#menu 
{
	border-bottom: 2px solid #312e28;
	margin: 0;
	padding-bottom: 19px;
	padding-left: 10px;
}

#menu ul, #menu li	
{
	display: inline;
	list-style-type: none;
	border: 0px;
	margin: 0;
	padding: 0;
}

#menu li.on a:link, #menu li.on a:visited
{
        background: #ffffff;
        border: 1px solid #244f75;
        float: left;
        font-weight: bold;
        line-height: 14px;
        margin-right: 8px;
        padding: 2px 10px 2px 10px;
        color: #244f75;
        text-decoration: none;
}

#menu li.on a:hover
{
}


#menu a:link, #menu a:visited	
{
	background: #312e28;
	border: 1px solid #244f75;
	float: left;
	font-weight: bold;
	line-height: 14px;
	margin-right: 8px;
	padding: 2px 10px 2px 10px;
	color: #ffffff;
	text-decoration: none;
}

#menu a:hover	
{
}


