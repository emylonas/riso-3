#searchbox
{
        background: #edddcb;
        width: 592px;
        border: 1px solid #ba9593;
        padding: 10px;
        margin-bottom: 10px;
        overflow: auto;
}

#searchform h2 {
	text-align: left;
	color: #752b27;
	padding-top: 4px;
	padding-bottom: 4px;
}

#menu 
{
	border-bottom: 2px solid #312e28;
	margin: 0;
	padding-bottom: 19px;
	padding-left: 10px;
}

#menu ul, #menu li	
{
	display: inline;
	list-style-type: none;
	border: 0px;
	margin: 0;
	padding: 0;
}

#menu li.on a:link, #menu li.on a:visited
{
        background: #ffffff;
        border: 1px solid #244f75;
        float: left;
        font-weight: bold;
        line-height: 14px;
        margin-right: 8px;
        padding: 2px 10px 2px 10px;
        color: #244f75;
        text-decoration: none;
}

#menu li.on a:hover
{
}


#menu a:link, #menu a:visited	
{
	background: #312e28;
	border: 1px solid #244f75;
	float: left;
	font-weight: bold;
	line-height: 14px;
	margin-right: 8px;
	padding: 2px 10px 2px 10px;
	color: #ffffff;
	text-decoration: none;
}

#menu a:hover	
{
}


